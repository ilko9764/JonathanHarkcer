<resources>>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
       <!-- <item name="android:actionBarStyle">@style/MyActionBar</item> -->
    </style>

    <style name="MyActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">#00000000</item>
    </style>

    <style name="oldManuskript">
        <item name = "android:layout_width">match_parent</item>
        <item name = "android:layout_height">wrap_content</item>
        <item name = "android:textAllCaps">false</item>
        <item name = "android:background">@drawable/border</item>
        <item name = "android:gravity">center</item>
        <item name = "android:textSize">25dp</item>
        <item name = "android:fontFamily">@font/manuskript</item>
    </style>

    <style name="oldManuskriptObj">
        <item name = "android:layout_width">match_parent</item>
        <item name = "android:layout_height">wrap_content</item>
        <item name = "android:textAllCaps">false</item>
        <item name = "android:background">@drawable/border</item>
        <item name = "android:gravity">center</item>
        <item name = "android:textSize">25dp</item>
        <item name = "android:fontFamily">@font/manuskript</item>
    </style>

    <style name="oldManuskriptEditView">
        <item name = "android:layout_width">fill_parent</item>
        <item name = "android:layout_height">wrap_content</item>
        <item name = "android:gravity">center</item>
        <item name = "android:textSize">25dp</item>
        <item name = "android:fontFamily">@font/manuskript</item>
    </style>
    <style name="oldManuskriptDialog">
        <item name = "android:layout_height">wrap_content</item>
        <item name = "android:layout_width">match_parent</item>
        <item name = "android:textAllCaps">false</item>
        <item name = "android:fontFamily">@font/manuskript</item>
        <item name = "android:background">@drawable/border</item>
        <item name = "android:textSize">25dp</item>
    </style>

    <style name="RoundeManuskript">
        <item name="android:layout_width">71dp</item>
        <item name="android:layout_height">71dp</item>
        <item name="android:fontFamily">@font/manuskript</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">20dp</item>
        <item name="android:background">@drawable/roundebutton</item>
        <item name="android:layout_gravity">bottom|right</item>
    </style>



</resources>
